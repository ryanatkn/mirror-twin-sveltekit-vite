{
  "name": "felt-template",
  "description": "a static website template with SvelteKit, Gro, and Felt",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 14.16.0"
  },
  "scripts": {
    "dev": "gro dev",
    "build": "gro build",
    "test": "gro test",
    "start": "gro start",
    "deploy": "gro deploy"
  },
  "devDependencies": {
    "@feltcoop/gro": "^0.21.5",
    "@sveltejs/adapter-static": "^1.0.0-next.7",
    "@sveltejs/kit": "^1.0.0-next.94",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.29.0",
    "svelte-preprocess-esbuild": "^2.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.0"
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "svelteSortOrder": "options-scripts-markup-styles",
    "svelteBracketNewLine": true,
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
